predecesseur = false;
successeur = false;
//  recherche des mesures suivante et prÃ©cÃ©dentes chrnologiquement pour SO2
for(MesureSO2 mesure : *capteur->RecupererMesuresSO2())
{

    if(!predecesseur && mesure.getDate() < instant)
    {
      mesurePrecedente = mesure;
      predecesseur = true;
    }
    else if( mesure.getDate() < instant && mesure.getDate() > mesurePrecedente)
    {
      mesurePrecedente = mesure;
    }

    if(!successeur && mesure.getDate() > instant)
    {
      mesureSuivante = mesure;
      successeur = true;
    }
    else if( mesure.getDate() > instant && mesure.getDate() < mesureSuivante)
    {
      mesureSuivante = mesure;
    }
}

// moyenne des deux
if (predecesseur && successeur)
{
  moySO2 = (mesureSuivante.Valeur() + mesurePrecedente.Valeur()) / 2.;
}
else if (predecesseur && ! successeur)
{
  moySO2 = mesurePrecedente;
}
else if (!predecesseur &&  successeur)
{
  moySO2 = mesurePrecedente;
}
else
{
  moySO2 = -1;
}
